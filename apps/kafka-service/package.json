{
  "name": "@./kafka-service",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "serve": {
        "executor": "nx:run-commands",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "command": "node apps/kafka-service/dist/main.js",
          "color": true,
          "parallel": false
        },
        "configurations": {
          "development": {
            "command": "node apps/kafka-service/dist/main.js"
          },
          "production": {
            "command": "node apps/kafka-service/dist/main.js"
          }
        }
      }
    }
  }
}
